    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.0",
    "prettier": "^2.3.2",
    "tslint-react": "^5.0.0",
    "typescript": "^4.4.2"
